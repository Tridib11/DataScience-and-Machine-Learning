class Student:
    def __init__(self,name,marks):
        self.name=name
        self.marks=marks
    def average(self):
        sum=0
        for i in self.marks:
            sum+=i
        print(f"Hi {self.name} Your average marks is {sum/3}")


student=Student("Tony Stark",[30,45,60])
student.average()

