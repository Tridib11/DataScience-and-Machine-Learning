def factorial(n):
    ans=1
    for i in range(1,n+1):
        ans*=i
    return ans
factorial(3)


from math import sqrt
def isPrime(n):
    for i in range(2,round(sqrt(n)+1)):
        if n%i==0:
            return False
        else:
            return True
